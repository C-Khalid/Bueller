<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Nick Name</th>
    <th>Email</th>
    <th>Avatar</th>
  </tr>
  <tbody>
  <% @students.each do |s| %>
  <tr>
    <td><%= s.name %>
    <td><%= s.nickname %></td>
    <td><%= s.email %></td>
    <td><%= image_tag s.imageurl.to_s+".jpg" , :width => 50, :height => 50 %></td>
  </tr>
  <% end %>
  </tbody>
</table>